@prefix bldg: <http://example.com/building#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .

bldg:AHU a brick:Air_Handling_Unit ;
    brick:feeds bldg:VAV_S .

bldg:VAV_S_T a brick:Supply_Air_Temperature_Sensor .

bldg:VAV_S a brick:Variable_Air_Volume_Box_With_Reheat ;
    brick:feeds bldg:ZONE_S ;
    brick:hasPart bldg:VAV_S_DPR ;
    brick:hasPoint bldg:VAV_S_FLOW,
        bldg:VAV_S_FSET .

bldg:VAV_S_DPR a brick:Damper ;
    brick:hasPoint bldg:VAV_S_DPRPOS .

bldg:VAV_S_DPRPOS a brick:Damper_Position_Setpoint .

bldg:VAV_S_FLOW a brick:Supply_Air_Flow_Sensor .

bldg:VAV_S_FSET a brick:Supply_Air_Flow_Setpoint .

bldg:ZONE_S a brick:HVAC_zone ;
    brick:hasPart bldg:office_S ;
    brick:isRegulatedBy bldg:thermostatS .

bldg:office_S a brick:Room .

bldg:thermostatS a brick:Thermostat .

